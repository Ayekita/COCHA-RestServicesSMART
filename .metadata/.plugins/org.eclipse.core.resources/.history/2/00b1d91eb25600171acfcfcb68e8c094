package com.cocha.lineacredito.test;

import java.sql.Connection;

import com.cocha.lineacredito.business.ConsultaLineaCredito;
import com.cocha.lineacredito.dto.datosSalidaDTO;
import com.cocha.lineacredito.utiles.imprimeSalida;

public class DoTest {
	public static void main(String[] args) {
		String nemote = "COPEC";
		Connection conJdbc = null;
		ConsultaLineaCredito consultaLineaCredito = new ConsultaLineaCredito();		
		datosSalidaDTO salida;
		try {
			//CONSULTA LINEA DE CREDITO
			salida = consultaLineaCredito.ejecucion(nemote, conJdbc);
			String titulo = imprimeSalida.titulos().toString();
			System.out.println(titulo);
			String texto = imprimeSalida.datos(salida).toString();
			System.out.println(texto);
			
			
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
