package com.cocha.lineacredito.business;

import java.sql.Connection;

import com.cocha.lineacredito.dao.SqlSmartDao;
import com.cocha.lineacredito.dto.datosSalidaDTO;

public class ConsultaLineaCredito {

	public datosSalidaDTO ejecucion (String nemote, Connection con) throws Exception {
		SqlSmartDao sqlSmartDao = new SqlSmartDao();
		datosSalidaDTO salida = sqlSmartDao.obtieneLineaCredito(nemote, con);
		return salida;
	}

}
